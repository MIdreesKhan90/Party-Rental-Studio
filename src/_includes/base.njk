  <body class="text-light">
    <div id="overlay"></div>
    <div id="toggleMenuContainer">
      <a href="#" class="toggle-nav">
        <span></span>
      </a>
    </div>
    <div id="primaryNav" class="primary-nav-wrapper">
      <div class="container-md">
        <div id="prs-logo-mobile">
          <a href="/">
            <img src="{{ '/assets/images/partyrentalstudio.svg' }}" width="230" height="55" alt="Party Rental Studio Logo">
          </a>
        </div>
        <nav class="primary-nav">
          <div class="left">
            <a class="brand-link" href="/">
              <div id="prs-logo">
                <img src="{{ '/assets/images/partyrentalstudio.svg' }}" width="230" height="55" alt="Party Rental Studio Logo">
              </div>
            </a>
          </div>
          <div class="right">
            <ul class="menu">
              <li class="active">
                <a class="nav-link" href="{{ '/' }}">Home</a>
              </li>
              <li>
                <a class="nav-link" href="{{ '/party-rental-website-design' }}">Features</a>
              </li>
              <li>
                <a class="nav-link" href="{{ '/pricing' }}">Pricing</a>
              </li>
              <li>
                <a class="nav-link" href="{{ '/customer-testimonials' }}">Testimonial</a>
              </li>
              <li>
                <a class="nav-link" href="{{ '/articles/1/' }}">Articles</a>
              </li>
              <li class="show-for-small-only">
                <a class="nav-link" href="{{ '/articles' }}">Careers</a>
              </li>
            </ul>
            <div class="menu-action-btns">
              <a href="{{ 'https://app.partyrentalstudio.com/' }}" class="btn btn-primary-outline">Sign In</a>
              <a href="{{ '/free-trial' }}" class="btn btn-primary">Free Trial</a>
            </div>
          </div>
        </nav>
      </div>
    </div>
    <div class="main">
      {% block content %}
        {{ content | safe }}
      {% endblock %}
    </div>



    <footer role="sitemap" class="footer-section">
      <div class="container-md">
        <div class="footer-contact">
          <div class="left">
            <img src="{{ '/assets/images/partyrentalstudio-footer.svg' }}" width="213" height="51" alt="Party Rental Studio Logo">
            <p class="left-desc">Everything you need to run a party rental business in one software.</p>
            <a href="mailto:support@partyrentalstudio.com">support@partyrentalstudio.com</a>
            <a href="tel:8006194568">(800) 619-4568</a>
          </div>
          <div class="right">
            <div>
              <h4 class="title">Site Map</h4>
              <ul class="footer-menu">
                <li>
                  <a href="/pricing">Pricing</a>
                </li>
                <li>
